{
  "name": "shorturl",
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "start": "node ./src/app production",
    "dev": "node ./src/app dev",
    "coverage": "jest --maxWorkers=4 --coverage"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-rate-limit": "^6.6.0",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@types/jest": "^29.1.2",
    "@types/supertest": "^2.0.12",
    "jest": "^29.2.0",
    "supertest": "^6.3.0"
  },
  "jest": {
    "verbose": true,
    "collectCoverageFrom": [
      "**/*.{js,ejs}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/coverage/"
    ]
  }
